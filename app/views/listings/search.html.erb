<ul>
  <% if @listings.present? %>
      <% @listings.each do |listing| %>
          <li>
            <span><%= listing.created_at.strftime('%B %d, %Y') %></span>
            <p><%= link_to listing.title, listing %></p>
          </li>
      <% end %>
  <% else %>
      <p>No listings found!</p>
  <% end %>
</ul>