<html>
<head></head>
<body>
<%= form_for @listing do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode, maxlength: 5 %>
    <%= f.submit %>
<% end %>
</body>
</html>